<?xml version="1.0"?>

<project name="genedb-libs" basedir=".">
	<description>This build file simply describe a set of 
	classpaths "exported" from this project, and common 
	properties</description>
	
	<!-- Paths/Properties -->
	<property name="spring-version" value="2.5.4"/>
	<property name="build.dir" value="./ant-build"/>
	<dirname property="imported.basedir" file="${ant.file.imported}"/>
    <property name="libDir" value="${imported.basedir}/lib" 

	<!-- Classpaths -->
	<path id="core-libs">
		<path location="${libDir}/spring-${version}.jar">
		<path location="${libDir}/spring-bundle-${version}/log4j/log4j-1.2.15.jar"/>
	</path>

</project>
