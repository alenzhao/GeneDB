<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
    <class name="org.genedb.prelim.orm.dao.Gene" table="GenePeri" proxy="org.genedb.prelim.orm.dao.Gene">
        <id name="geneId" type="int" column="geneId">
	    <generator class="native" />
        </id>	
        <property name="systematicId" type="string" column="name" />
        <property name="geneName" type="string" column="genename" />
        <property name="chromosome" type="string" />
	<property name="molecularWeight" type="double" />
	<property name="type" type="string" column="gene_type"/>
	<property name="numExons" type="int" column="number_of_exons" />
	<property name="TMM" type="int" column="number_of_tmhmm" />
	<property name="proteinLength" type="int" />
	<property name="GPI" type="boolean" column="has_dgpi" />
	<property name="signalP" type="boolean" column="has_signalp" />
	<property name="anyPfam" type="boolean" />
	<property name="anyGO" type="boolean" />
	<property name="curation" type="string" length="3999" />
	<property name="product" type="string" length="2048"/>
	<property name="ecNums" type="string" />
	<property name="allNames" type="string" />
	<property name="status" type="string" />
	<property name="algorithm" type="string" />
	<property name="nickname" type="string" />
	<property name="tissueType" type="string" />
	
	<many-to-one name="taxon" class="org.genedb.prelim.orm.dao.Taxon" column="taxonId" index="peri_taxon_idx" />

	<set name="pfams" table="Gene_Pfam" cascade="all">
	  <key column="geneId" />
      <many-to-many column="pfamId" class="org.genedb.prelim.orm.dao.Pfam" />
    </set>

	<many-to-one name="riley" class="org.genedb.prelim.orm.dao.Riley" column="rileyId" index="peri_riley_idx" />

	<set name="goEntries" table="Gene_Go" cascade="all">
	  <key column="geneId" />
      <many-to-many column="goId" class="org.genedb.prelim.orm.dao.CombinedGo" />
    </set>


</class>
</hibernate-mapping>
	